TARGETNAME=installer_x64
TARGETTYPE=PROGRAM

_NT_TARGET_VERSION= $(_NT_TARGET_VERSION_WINXP)

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF

USE_MSVCRT=1

UMTYPE=console
UMBASE=0x01000000

